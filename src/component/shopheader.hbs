<section>
  <header class='card row col-12'>
    {{#with navigation}}
      <div class="row">
        <div class="col-7 pt-2">{{Logo}}</div>
        <div class='nav col-5'>
          {{#each links}}
            <div class='nav-item'>
              <a href='{{href}}' class='nav-link {{#if @last}}disabled{{/if}}'>{{text}}</a>
            </div>
          {{/each}}
        </div>
      </div> 
    {{/with}}
</header>
</section>
